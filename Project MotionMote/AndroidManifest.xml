    <uses-permission android:name="android.permission.PACKAGE_USAGE_STATS"/>
    
    <uses-permission android:name="com.android.launcher.permission.READ_SETTINGS" />
    <uses-permission android:name="com.android.launcher.permission.WRITE_SETTINGS" />
    
	
	
	<uses-permission android:name="android.permission.WAKE_LOCK"></uses-permission>
    
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".MMoteMenuActivity"
                  android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

<activity android:name="MMoteGameControllerActivity" android:screenOrientation="landscape"></activity>
<activity android:name="MMoteAboutActivity"></activity>

<activity android:name="MMoteSettingsActivity">
            <intent-filter>
                <action android:name="android.intent.action.main" />
            </intent-filter>
</activity>
<activity android:name="MMoteWhiteBoardActivity"></activity>
<service android:name="MMoteService" android:label="MMote"></service>
</application>
    <uses-sdk android:minSdkVersion="7" />




<uses-permission android:name="android.permission.CHANGE_WIFI_STATE"></uses-permission>
<uses-permission android:name="android.permission.ADD_SYSTEM_SERVICE"></uses-permission>
</manifest> 